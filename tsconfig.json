{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./src",
        "skipLibCheck": true,
        "strict": true,
        "target": "ES6"
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src"
    ]
}